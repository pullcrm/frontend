{
  "name": "pullcrm",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Bohdan Tsaryk <bogdan.tsarik@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "QENV=development quasar dev -m ssr",
    "dev:ios": "QENV=development quasar dev -m capacitor -T ios",
    "build": "QENV=production quasar build -m ssr",
    "build:ios": "QENV=production quasar build -m capacitor -T ios",
    "build:pwa": "QENV=production quasar build -m pwa",
    "start": "node dist/ssr/index.js",
    "build:widget": "tsc 'public/static/widgets/widget.esnext.js' --allowJs --target 'ES5' --outFile 'public/static/widgets/widget.compiled.js'"
  },
  "dependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@quasar/extras": "^1.14.0",
    "@quasar/icongenie": "^2.5.2",
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "@vuepic/vue-datepicker": "^3.2.1",
    "@vueuse/core": "^8.7.4",
    "animated-scroll-to": "^2.2.0",
    "dayjs": "^1.11.3",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "pinia": "^2.0.14",
    "quasar": "^2.7.3",
    "raf": "^3.4.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vue-slicksort": "^2.0.0-alpha.5",
    "vue3-portal": "^1.0.3"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.2",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.21",
    "@types/raf": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-vue-components": "^0.19.6",
    "workbox-build": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
