{
  "name": "pullcrm",
  "version": "0.0.4",
  "private": true,
  "description": "Appointment Scheduling Software",
  "keywords": [
    "vue",
    "vue3",
    "quasar",
    "capacitor",
    "open-source"
  ],
  "homepage": "https://github.com/pullcrm/frontend#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/pullcrm/frontend.git"
  },
  "license": "MIT",
  "author": {
    "name": "Bohdan Tsaryk",
    "email": "bogdan.tsaryk@gmail.com",
    "url": "https://twitter.com/bruce_developer"
  },
  "scripts": {
    "build": "QENV=production quasar build -m ssr",
    "build:ios": "QENV=production quasar build -m capacitor -T ios -ide",
    "build:pwa": "QENV=production quasar build -m pwa",
    "build:widget": "tsc 'public/static/widgets/widget.esnext.js' --allowJs --target 'ES5' --outFile 'public/static/widgets/widget.compiled.js'",
    "dev": "QENV=development quasar dev -m ssr",
    "dev:ios": "QENV=development quasar dev -m capacitor -T ios",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "start": "node dist/ssr/index.js"
  },
  "dependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@capacitor/core": "^3.7.0",
    "@capacitor/storage": "^1.2.5",
    "@quasar/extras": "^1.15.2",
    "@quasar/icongenie": "^2.5.3",
    "@vuelidate/core": "^2.0.0-alpha.44",
    "@vuelidate/validators": "^2.0.0-alpha.31",
    "@vuepic/vue-datepicker": "^3.4.7",
    "@vueuse/core": "^9.1.1",
    "animated-scroll-to": "^2.3.0",
    "dayjs": "^1.11.5",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "pinia": "^2.0.21",
    "quasar": "^2.7.7",
    "raf": "^3.4.1",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue-slicksort": "^2.0.0-alpha.5",
    "vue3-portal": "^1.0.3"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.6",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.20.21",
    "@types/raf": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.4.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "productName": "pullcrm"
}
